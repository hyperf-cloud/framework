{
    "name": "hyperf/framework",
    "description": "A coroutine framework that focuses on hyperspeed and flexible, specifically use for build microservices and middlewares.",
    "license": "Apache-2.0",
    "keywords": [
        "php",
        "swoole",
        "framework",
        "hyperf",
        "microservice",
        "middleware"
    ],
    "support": {
    },
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.3",
        "fig/http-message-util": "^1.1.2",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0.1",
        "psr/http-server-middleware": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "psr/log": "^1.0",
        "symfony/console": "^4.2",
        "symfony/routing": "^4.2",
        "symfony/http-foundation": "^4.2",
        "hyperf/contract": "dev-master",
        "hyperf/dispatcher": "dev-master",
        "hyperf/memory": "dev-master",
        "hyperf/utils": "dev-master",
        "vlucas/phpdotenv": "^3.1"
    },
    "require-dev": {
        "malukenho/docheader": "^0.1.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0.0",
        "friendsofphp/php-cs-fixer": "^2.9",
        "symfony/console": "^4.1",
        "php-di/php-di": "^6.0",
        "doctrine/common": "@stable"
    },
    "suggest": {
    },
    "autoload": {
        "files": [
        ],
        "psr-4": {
            "Hyperf\\Framework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
        },
        "hyperf": {
            "config": "Hyperf\\Framework\\ConfigProvider"
        }
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
